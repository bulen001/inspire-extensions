<?xml version="1.0" ?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:cp="http://inspire.ec.europa.eu/schemas/cp/4.0"
           xmlns:ex="http://www.wetransform.to/ie-examples/mixin/1.0"
           elementFormDefault="qualified"
           targetNamespace="http://www.wetransform.to/ie-examples/mixin/1.0"
           version="1.0">
    <xs:import namespace="http://inspire.ec.europa.eu/schemas/cp/4.0" schemaLocation="http://inspire.ec.europa.eu/schemas/cp/4.0/CadastralParcels.xsd"/>
    <xs:element name="JoinedParcel" substitutionGroup="cp:CadastralParcel" type="ex:JoinedParcelType"/>
    <xs:complexType name="JoinedParcelType">
        <xs:complexContent>
            <xs:extension base="cp:CadastralParcelType">
                <xs:sequence>
                    <xs:element name="joinedFrom" type="cp:CadastralParcelPropertyType" minOccurs="1" maxOccurs="unbounded"/>
                    <xs:element name="beginLifespan" nillable="true">
                        <xs:complexType>
                            <xs:simpleContent>
                                <xs:extension base="xs:dateTime" />
                            </xs:simpleContent>
                        </xs:complexType>
                    </xs:element>
                    <xs:element name="endLifespan" nillable="true">
                        <xs:complexType>
                            <xs:simpleContent>
                                <xs:extension base="xs:dateTime" />
                            </xs:simpleContent>
                        </xs:complexType>
                    </xs:element>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
</xs:schema>